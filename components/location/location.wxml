<view class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd weui-cell_primary">
        <view>地址</view>
    </view>
    <view class="weui-cell__ft">
        <view class='address' bindtap='onChangeAddress' style="font-size:28rpx">
            <text class="iconfont icon-location dark" style="font-size:28rpx"></text> \t
            <text class="dark">{{address || '定位'}}</text>
        </view>
    </view>
</view>
<map id="qqMap" style="width: 100%;height:0" latitude="{{latitude}}" longitude="{{longitude}}" show-location></map>