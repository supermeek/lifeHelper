<block wx:for="{{listData}}" wx:key="index">
    <!-- 菜单链接 -->
    <navigator url="{{item.url}}" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd" wx:if="{{item.icon}}">
            <text class="{{item.icon}} cyan"></text>
        </view>
        <view class="weui-cell__bd weui-cell_primary">
            <view>{{item.name}}</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access">{{item.info}}</view>
    </navigator>
</block>
