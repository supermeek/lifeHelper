
<view class="weui-cell weui-cell_example weui-cell_access">
    <view class="weui-cell__bd weui-cell_primary">
        <view>{{item.name}}</view>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access" style="min-width:400rpx;">
        <input 
            class="weui-input dark" 
            type="text"
            name="{{item.key}}"
            value="{{value.name}}"
            data-name="{{item.name}}"
            placeholder-class="gray" 
            placeholder="请输入{{item.name}}"
            bindblur="bindblur"
            bindconfirm="bindblur"
            bindinput="binInput" />
    </view>
</view>
<view class="weui-cell search-list__wrapper" style="display:block"> 
    <block wx:for="{{resultList}}" wx:key="index">
        <view 
            data-name="{{item.name}}" 
            data-address="{{item.address}}" 
            data-exist="{{item.name}}" 
            bindtap="reslutClick">  
            <rich-text nodes="{{item.label}}"></rich-text>
        </view>
    </block>
</view>
