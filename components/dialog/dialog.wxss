@import '../../app.wxss';
.weui-demo-dialog{
  visibility:hidden;
  opacity:0;
  transition:opacity .2s;
   z-index:99999;
}
.weui-demo-dialog_show{
  visibility:visible;
  opacity:1;
}
.weui-dialog__bd{
  padding: 30rpx 0;
  box-sizing: border-box;
  margin: 0;
}
.weui-dialog__hd{
    min-height: 100rpx !important;
    box-sizing: border-box;
    padding: 60rpx 24px 0;
}
.weui-dialog__ft{
    min-height: 120rpx !important;
    line-height: 120rpx;
}